{
  "name": "deepproxy",
  "version": "2.0.0",
  "description": "A HTTP Proxy with Recording and Replay modes",
  "main": "src/main.js",
  "scripts": {
    "start": "node src/main.js",
    "start:old": "node src/index.js",
    "dev": "nodemon src/main.js",
    "db:init": "node src/database/init.js",
    "test": "npm run test:unit && npm run test:integration",
    "test:all": "jest --coverage",
    "test:unit": "jest tests/unit --coverage",
    "test:integration": "jest tests/integration",
    "test:e2e": "jest tests/e2e",
    "test:matching": "jest tests/integration/matching",
    "test:modes": "jest tests/integration/modes",
    "test:sessions": "jest tests/integration/sessions",
    "test:https": "jest tests/integration/https",
    "test:api": "jest tests/integration/api",
    "test:watch": "jest --watch",
    "test:verbose": "jest --verbose --coverage",
    "test:coverage": "jest --coverage && open coverage/lcov-report/index.html",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "web:dev": "cd web && npm run dev",
    "web:build": "cd web && npm run build",
    "docker:build": "docker build -t dproxy:latest .",
    "docker:run": "docker run -p 8080:8080 -p 3080:3080 dproxy:latest"
  },
  "keywords": [
    "proxy",
    "mobile",
    "http",
    "cache",
    "recording",
    "replay",
    "testing"
  ],
  "author": "Tony XU <fihtony@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.13.2",
    "better-sqlite3": "^11.7.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.3",
    "morgan": "^1.10.0",
    "node-forge": "^1.3.3",
    "sqlite": "^5.1.1",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.5",
    "@types/jest": "^30.0.0",
    "eslint": "^8.56.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.1",
    "https-proxy-agent": "^7.0.6",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "sqlite3": "^5.1.7",
    "supertest": "^6.3.4",
    "tunnel": "^0.0.6"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
